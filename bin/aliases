vr=$HOME/vreports; alias vr='cd $vr'
vrb=$vr/bin      ; alias vrb='cd $vrb'
vre=$vr/examples ; alias vre='cd $vre'
vreb=$vre/books  ; alias vreb='cd $vreb'
vrebs=$vreb/sql  ; alias vrebs='cd $vrebs'
vrebsd=$vrebs/data; alias vrebsd='cd $vrebsd'
vrl=$vr/lib      ; alias vrl='cd $vrl'
vrw=$vr/web      ; alias vrw='cd $vrw'
vrwc=$vrw/css    ; alias vrwc='cd $vrwc'

# database
alias vrdb='mysql -uvreports -pwoodenships60 --database=vreports'